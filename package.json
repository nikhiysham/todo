{
  "name": "todo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "node_modules/.bin/eslint src/",
    "lintfix": "node_modules/.bin/eslint --fix src/; exit 0",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "android:release": "cd android && ./gradlew assembleRelease",
    "android:prod": "react-native run-android --variant=release",
    "cache:clear": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-*"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "color": "^3.0.0",
    "lodash": "^4.17.13",
    "react": "16.8.3",
    "react-native": "0.59.10",
    "react-native-config": "^0.11.5",
    "react-native-gesture-handler": "1.1.0",
    "react-native-screens": "1.0.0-alpha.7",
    "react-native-splash-screen": "3.0.9",
    "react-navigation": "^3.9.1",
    "react-navigation-redux-helpers": "^2.0.5",
    "react-redux": "^5.0.7",
    "reactotron-react-native": "^3.2.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "@babel/plugin-external-helpers": "^7.2.0",
    "babel-eslint": "^9.0.0",
    "babel-preset-react-native": "^5.0.1",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-sorting": "^0.3.0",
    "metro-react-native-babel-preset": "^0.53.1",
    "react-test-renderer": "16.4.1",
    "schedule": "0.4.0"
  },
  "resolutions": {
    "stacktrace-parser": "0.1.4"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts/poppins"
    ]
  }
